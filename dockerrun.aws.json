{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
        {
            "name" : "client",
            "image" : "jsaurabh95/multi-client",
            "hostname": "client",
            "essential" : false
        },
        {
            "name" : "server",
            "image" : "jsaurabh95/multi-server",
            "hostname" : "api",
            "essential" : false
        },
        {
            "name" : "worker",
            "image" : "jsaurabh95/multi-worker",
            "hostname" : "worker",
            "essential": false
        },
        {
            "name" : "nginx",
            "image": "jsaurabh95/multi-nginx",
            "hostname" : "nginx",
            "essential": true,
            "memory" : 128,
            "portMappings" : [
                {
                "hostPort":80,
                "containerPort":80
                }
            ],
            "links" : ["client", "server"]
        }
    ]
}